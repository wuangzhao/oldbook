<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.fd.dao.BuyDao">

    <insert id="insert" parameterType="org.fd.entity.BuyEntity">
        INSERT INTO buy(user_id, sell_id,buy_id) VALUES (#{userId}, #{sellId},#{buyId})
    </insert>

    <!--没写全-->
    <select id="queryByUser" resultType="org.fd.entity.BuyEntity" parameterType="org.fd.entity.UserEntity">
        SELECT buy_id,sell_id,user_id FROM buy WHERE user_id = #{userId}
    </select>

</mapper>